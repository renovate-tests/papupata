    "gatsby-transformer-remark": "3.0.0",
    "gatsby-transformer-sharp": "3.0.0",
    "modern-normalize": "1.0.0",
    "normalize.css": "8.0.1",
    "polished": "4.1.1",
    "prism-themes": "1.5.0",
